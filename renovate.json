{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "osvVulnerabilityAlerts": true,
  "labels": ["dependencies"],
  "prHourlyLimit": 5,
  "prConcurrentLimit": 50,
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "description": "Ignore org.apache.axis2 packages (produced by this repo)",
      "matchPackagePrefixes": ["org.apache.axis2"],
      "enabled": false
    }
  ]
}
